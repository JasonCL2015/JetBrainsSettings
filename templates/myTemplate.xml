<templateSet group="myTemplate">
  <template name="pvm" value="public void $VAR$() throws Exception {&#10;}" description="create a void method" toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="prs" value="private String $VAR$;" description="create private String" toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
      <option name="JAVA_STRING" value="false" />
    </context>
  </template>
  <template name="mc" value="/**&#10; * &lt;p&gt; $description$ &lt;/p&gt;&#10;        $param$&#10; * @return $return$&#10; * @throws $throws$&#10; * @author Jason.C&#10; * @date $date$ $time$&#10; * @since V1.1.0-SNAPSHOT&#10; */" description="方法注释" toReformat="false" toShortenFQNames="true">
    <variable name="description" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="param" expression="groovyScript(&quot;groovy code&quot;)" defaultValue="" alwaysStopAt="true" />
    <variable name="return" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="throws" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="date" expression="date()" defaultValue="" alwaysStopAt="true" />
    <variable name="time" expression="time()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="cc" value="/**&#10; * &lt;p&gt; $description$ &lt;/p&gt;&#10; * @author Jason.C&#10; * @date $date$ $time$&#10; * @since V1.1.0-SNAPSHOT&#10; */" description="类注释" toReformat="false" toShortenFQNames="true">
    <variable name="description" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="date" expression="date()" defaultValue="" alwaysStopAt="true" />
    <variable name="time" expression="time()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>